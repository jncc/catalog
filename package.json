{
  "name": "catalog",
  "version": "1.0.0",
  "description": "product catalog",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf built",
    "compile": "tsc --outDir built",
    "build-api": "yarn run clean && yarn run compile",
    "dev": "yarn run build && nodemon built/server.js",
    "test": "mocha -r ts-node/register \"api/**/*.ts\"",
    "build-docs": "python -msphinx docs-source docs",
    "build": "yarn run build-api && yarn run build-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jncc/catalog.git"
  },
